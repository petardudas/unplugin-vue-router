{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./env.d.ts",
    "./src/**/*.ts",
    "./src/**/*.vue",
    "./typed-router.d.ts",
    "./auto-imports.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "unplugin-vue-router/runtime": [
        "../src/runtime"
      ],
      "unplugin-vue-router/types": [
        "../src/types"
      ],
    }
  },
  "vueCompilerOptions": {
    "plugins": [
      "../volar"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
